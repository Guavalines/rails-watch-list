<h1>Create your new Bookmark!</h1>

    <%= simple_form_for [ @list, @bookmark ] do |f| %>
    <div class="form-inputs">
      <%= f.input :comment %>
      <%= f.association :movie %>
    </div>
    <div class="form-actions">
      <%= link_to "Cancel", :back, class:"btn btn-outline-warning" %>
      <%= f.button :submit, class:"btn btn-outline-dark" %>
    </div>
    <% end %>
